p=[0]*3  #3개의 파스타를 위한 배열 공간 설정
j=[0]*2  #2개의 주스를 위한 배열 공간 설정
for i in range(3):  #3번 반복
    p[i]=int(input())  #파스타 입력
    
for i in range(2):  #2번 반복
    j[i]=int(input())  #주스 입력

min_p=min(p)  #파스타 중 가장 작은 가격 찾기
min_j=min(j)  #주스 중 가장 작은 가격 찾기


print((min_p+min_j)+((min_p+min_j)/10))  #가장 작안 파스타 가격과 주스 가격을 더한 것에 10% 더한 만큼 출력

