#include<stdio.h>
int main()
{
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
    
    if(a==b&&a==c)
    {
        printf("yes");
        return 0;
    }
    
    if(a>=b&&a>=c)
        if(a<b+c) printf("yes");
        else printf("no");
        
    if(b>=a&&b>=c)
        if(b<a+c) printf("yes");
        else printf("no");
        
    if(c>=a&&c>=b)
        if(c<a+b) printf("yes");
        else printf("no");
    
}
