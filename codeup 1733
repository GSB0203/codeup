#include<stdio.h>
int main()
{
    char n[100]={};
    int i;
    
    scanf("%s", &n);
    
    for(i=0;n[i]!='\0';i++)
    {
        if(n[i]=='I'&&n[i+1]=='O'&&n[i+2]=='I'&&n[i+3]=='\0')
        {
            printf("IOI is the International Olympiad in Informatics.");
            break;
        }
        else
        {
            printf("I don't care.");
            break;
        }
    }
}
