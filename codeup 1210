#include<stdio.h>
int main()
{
    int a, b, s, s1;
    scanf("%d %d", &a, &b);
    
    if(a==1) s=400;
    else if(a==2) s=340;
    else if(a==3) s=170;
    else if(a==4) s=100;
    else if(a==5) s=70;
    
    if(b==1) s1=400;
    else if(b==2) s1=340;
    else if(b==3) s1=170;
    else if(b==4) s1=100;
    else if(b==5) s1=70;
    
    if(s+s1>500) printf("angry");
    else printf("no angry");
}
