#include<stdio.h>
int main()
{
    int a, b, i;
    scanf("%d %d", &a, &b);
    for(i=1; i<b; i++)
        if(a*i==b)
        {
            printf("%d*%d=%d", a, i, b);
            return 0;
        }
        
    for(i=1; i<a; i++)
        if(b*i==0)
        {
            printf("%d*%d=%d", b, i, a);
            return 0;
        }
    printf("none");
}
