#include<stdio.h>
int main()
{
    int i, num, n, m, arr[10000000]={};
    scanf("%d", &n);
    
    for(i=0;i<n;i++)
    {
        scanf("%d", &num);
        arr[num]=1;
    }
    
    scanf("%d", &m);
    
    for(i=0;i<m;i++)
    {
        scanf("%d", &num);
        printf("%d ", arr[num]);
    }
}
